#include <iostream>

using namespace std;

struct candy {
    char name[25];
    double mass;
    double calories;

    void print() {
        cout << name << "\n" << mass << "\n" << calories << endl;
    }

};

int main() {
    candyBar snack[3] = {{"Mocha", 2.3, 350},
                         {"Mas",        2.1, 400},
                         {"Snik",    4.5, 345}};
    for (int i = 0; i <= 2; i++)
        snack[i].print();
    return 0;
}
